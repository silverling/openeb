{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "openeb",
  "version": "5.0.0",
  "dependencies": [
    "boost",
    "glew",
    "glfw3",
    {
      "name": "hdf5",
      "features": [
        "cpp"
      ]
    },
    {
      "name": "libusb",
      "features": [
        "udev"
      ]
    },
    {
      "name": "opencv4",
      "default-features": false,
      "features": [
        "calib3d",
        "highgui",
        "jpeg",
        "png"
      ]
    },
    "protobuf",
    "pybind11"
  ]
}
